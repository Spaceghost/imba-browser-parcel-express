{
  "name": "imba-browser-parcel-express",
  "version": "1.0.0",
  "description": "Simple Node Sandbox",
  "main": "src/index.js",
  "scripts": {
    "start": "npm-run-all --parallel imba devserv",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "imbac --output source/scripts --target web imba && parcel build source/index.html --out-dir release",
    "devserv": "imbac --output src/index.js --target node src/index.imba && parcel source/index.html --out-dir .cache",
    "imba": "imbac --watch --output source/scripts --target web imba",
    "dev": "npm-run-all --parallel imba devserv"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Spaceghost/imba-browser-parcel-express.git"
  },
  "dependencies": {},
  "devDependencies": {
    "imba": "^1.4.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.9.7",
    "parcel-plugin-clean-dist": "^0.0.6",
    "nodemon": "1.18.4"
  }
}
